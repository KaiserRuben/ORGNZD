<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ORGNZD.ORGNZD" version="0.1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ORGNZD.</name>
    <description>
        This is the App u gonna use all day to organise urself!
    </description>
    <author email="Ruben.Kaiser@t-online.de" href="https://orgnzd.lennartvonwerder.de/">
        ORGNZD.-Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="data:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarBackgroundColor" value="#fcb8b8" />
</widget>
